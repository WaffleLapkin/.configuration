{
    "rust-analyzer.check.invocationLocation": "root",
    "rust-analyzer.check.invocationStrategy": "once",
    "rust-analyzer.check.overrideCommand": [
        "python3",
        "x.py",
        "check",
        "--json-output",
        "--build-dir",
        "build/rust-analyzer"
    ],
    "rust-analyzer.linkedProjects": [
        // "src/bootstrap/Cargo.toml",
        "Cargo.toml"
    ],
    "rust-analyzer.rustfmt.overrideCommand": [
        "${workspaceFolder}/build/host/rustfmt/bin/rustfmt",
        "--edition=2021"
    ],
    "rust-analyzer.procMacro.server": "${workspaceFolder}/build/host/stage0/libexec/rust-analyzer-proc-macro-srv",
    "rust-analyzer.procMacro.enable": true,
    "rust-analyzer.cargo.buildScripts.enable": true,
    "rust-analyzer.cargo.buildScripts.invocationLocation": "root",
    "rust-analyzer.cargo.buildScripts.invocationStrategy": "once",
    "rust-analyzer.cargo.buildScripts.overrideCommand": [
        "python3",
        "x.py",
        "check",
        "--json-output",
        "--build-dir",
        "build/rust-analyzer"
    ],
    "rust-analyzer.cargo.sysrootSrc": "./library",
    "rust-analyzer.rustc.source": "./Cargo.toml",
    "editor.formatOnSave": true,
    // https://rust-lang.zulipchat.com/#narrow/stream/185405-t-compiler.2Frust-analyzer/topic/How.20to.20fix.20.60UnsupportedABI.60.20for.20custom.20toolchains.3F/near/299041266
    "rust-analyzer.server.extraEnv": {
        "RA_LOG": "warn,rust_analyzer::reload=debug",
    },
    "cSpell.words": [
        "Expn",
        "Rptr",
        "Substs",
        "TMPDIR",
        "Zmiri",
        "coinductive",
        "ctxt",
        "fdiv",
        "intravisit",
        "libcore",
        "mois",
        "monomorphization",
        "mplace",
        "qpath",
        "reinit",
        "reinits",
        "selcx",
        "sess",
        "substs",
        "subtyping",
        "sugg",
        "supertraits",
        "thir",
        "uncategorized",
        "upvars",
        "worklist"
    ]
}